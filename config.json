  "extensions": {
    "update_url": "https://www.perplexity.ai/rest/browser/update-crx?channel=stable",
    "fetch_from_server": true,
    "bundled": {
      "perplexity": {
        "id": "mcjlamohcooanphmebaiigheeeoplihb",
        "name": "Perplexity",
        "fallback_version": "1.0.64"
      },
      "comet_web_resources": {
        "id": "mjdcklhepheaaemphcopihnmjlmjpcnh",
        "name": "Comet Web Resources",
        "fallback_version": "2025.12.19.1204"
      },
      "agents": {
        "id": "npclhjbddhklpbnacpjloidibaggcgon",
        "name": "Agents",
        "fallback_version": "0.0.165"
      }
    },
    "patch_config": {
      "perplexity": {
        "manifest_additions": {
          "declarative_net_request": {
            "rule_resources": [
              {
                "id": "meteor_telemetry",
                "enabled": true,
                "path": "rules/telemetry.json"
              }
            ]
          },
          "content_scripts": [
            {
              "matches": [
                "https://www.perplexity.ai/*",
                "https://perplexity.ai/*"
              ],
              "js": [
                "content/content-script.js"
              ],
              "run_at": "document_start",
              "world": "MAIN",
              "all_frames": true
            }
          ]
        },
        "service_worker_import": "meteor-prefs.js",
        "copy_files": {
          "meteor-prefs.js": "perplexity/meteor-prefs.js",
          "rules/telemetry.json": "perplexity/telemetry.json",
          "content/content-script.js": "perplexity/content-script.js"
        },
        "file_patches": {
          "_comment": "Regex replacements applied to existing extension files. Patterns are actual regex.",
          "assets/js/background.ts.*.js": [
            {
              "pattern": "isDebugMode:\\w+!==\"production\"",
              "replacement": "isDebugMode:true",
              "description": "Force AnalyticsService debug mode to skip fetch calls"
            },
            {
              "pattern": "async fetchFlagConfigurations\\(\\)\\{if\\(!this\\.configurationRequestParameters\\)",
              "replacement": "async fetchFlagConfigurations(){return;if(!this.configurationRequestParameters)",
              "description": "Disable Eppo SDK fetch - return immediately to prevent network errors"
            },
            {
              "pattern": "async init\\(\\)\\{if\\(!await this\\.#s\\(\\)\\)\\{this\\.logger\\.info\\(\"Zero suggest pref is unavailable",
              "replacement": "async init(){return;if(!await this.#s()){this.logger.info(\"Zero suggest pref is unavailable",
              "description": "Disable SpotlightZeroSuggestUpdater - return immediately"
            },
            {
              "pattern": "async shouldSendStartupMetrics\\((\\w+)\\)\\{try\\{const (\\w+)=await this\\.browserStorage\\.getItem\\(",
              "replacement": "async shouldSendStartupMetrics($1){return false;try{const $2=await this.browserStorage.getItem(",
              "description": "Disable StartupMetricsManager - always return false"
            }
          ]
        }
      },
      "comet_web_resources": {
        "file_patches": {
          "_comment": "Regex replacements for comet_web_resources extension files",
          "spa/assets/ask-input-*.js": [
            {
              "pattern": "(\\w+)\\.getQuickActions\\(\"get-quick-actions-for-sidecar-suggests\"\\)",
              "replacement": "Promise.resolve([])",
              "description": "Disable quick actions in spa - return empty array immediately"
            }
          ],
          "sidecar/assets/index-*.js": [
            {
              "pattern": "(\\w+)\\.getQuickActions\\(\"get-quick-actions-for-sidecar-suggests\"\\)",
              "replacement": "Promise.resolve([])",
              "description": "Disable quick actions in sidecar - return empty array immediately"
            }
          ],
          "spa/assets/page-component-*.js": [
            {
              "pattern": "\\{widgetEditMode:(\\w+)\\}\\)\\{const",
              "replacement": "{widgetEditMode:$1}){return null;const",
              "description": "Disable privacy widget component - returns null to render nothing"
            },
            {
              "pattern": "\\{cometBrowser:(\\w+),type:(\\w+),locale:(\\w+)\\}\\)=>\\{const\\{data:(\\w+)=\\[\\]\\}",
              "replacement": "{cometBrowser:$1,type:$2,locale:$3})=>{return [];const{data:$4=[]}",
              "description": "Disable personal suggestions hook - returns empty array immediately"
            },
            {
              "pattern": "memo\\(\\(\\{context:(\\w+),widgetId:(\\w+),defaultName:(\\w+),defaultQuery:(\\w+),showSettings:(\\w+),setShowSettings:(\\w+)\\}\\)=>\\{",
              "replacement": "memo(({context:$1,widgetId:$2,defaultName:$3,defaultQuery:$4,showSettings:$5,setShowSettings:$6})=>{return null;",
              "description": "Disable ShortcutWidget component - returns null to render nothing"
            }
          ]
        }
      }
    }
  },
